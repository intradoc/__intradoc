{
  "name": "intradoc",
  "version": "0.0.0-development",
  "description": "Update your docs in-place.",
  "author": "Richard King <richrdkng@gmail.com> (https://www.richrdkng.com)",
  "license": "ISC",
  "main": "src/index.js",
  "bin": "src/cli.js",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intradoc/intradoc.git"
  },
  "bugs": {
    "url": "https://github.com/intradoc/intradoc/issues"
  },
  "homepage": "https://intradoc.github.io",
  "keywords": [
    "update",
    "doc",
    "docs",
    "inplace",
    "in-place",
    "in place",
    "util",
    "utils",
    "intradoc"
  ],
  "scripts": {
    "lint": "tsc --noEmit",
    "test": "jest",
    "prep:1": "rm -rf .dist && mkdir .dist",
    "prep:2": "cp ../../LICENSE* README* package.json .dist",
    "prep": "npm run prep:1 && npm run prep:2",
    "build": "tsc --project tsconfig.build.json"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node16": "^1.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.1",
    "fs-extra": "^10.1.0",
    "jest": "^28.1.2",
    "semantic-release": "^18.0.1",
    "semantic-release-monorepo": "^7.0.5",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@intradoc/cli": "^1.0.0",
    "@intradoc/core": "^2.0.0"
  }
}
